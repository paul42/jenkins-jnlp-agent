# defaults for jenkins-agent component of the jenkins continuous integration 
# system

# jenkins /run location
JENKINS_RUN="{{ jenkins_agent_run }}"

# URL of jenkins server to connect to 
# Not specifying this parameter will stop the agent 
# job from running.
JENKINS_URL="{{ jenkins_master }}"

# Node name
JENKINS_NODE_NAME="{{ jenkins_agent_name }}"

# Secret key to access jenkins server
JENKINS_SECRET="{{ jenkins_agent_secret }}"